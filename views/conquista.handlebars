<h1>Conquistas Cadastradas</h1>

<a href="/jogos/{{jogo.id}}/novaConquista">Cadastrar conquista</a>

<table border="1">
  <!-- table row -->
  <tr>
    <!-- table head -->
    <th>Titulo</th>
    <th>Descrição</th>
  </tr>
  {{#each conquista}}
    <tr>
      <td>{{this.titulo}}</td>
      <td>{{this.descricao}}</td>
    </tr>
  {{/each}}
</table>