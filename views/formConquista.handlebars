<h1> Cadastrar Conquista</h1>
<form action="/jogos/{{usuario.id}}/novaConquista" method="post">

  <div>
    <label for="conquista"></label>
    <input
      type="text"
      maxlength="16"
      minlength="16"
      id="numero"
      name="numero"
    />
  </div>
  <div>
    <label for="nome">Nome</label>
    <input type="text" id="nome" name="nome" />
  </div>
  <div>
    <label for=""> </label>
    <input type="number" id="" name="" />
  </div>

  <input type="submit" value="Cadastrar" />

</form>